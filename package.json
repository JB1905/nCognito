{
  "name": "nCognito",
  "version": "1.0.0",
  "description": "Cross browser incognito mode extension",
  "author": "Jakub Biesiada",
  "license": "MIT",
  "scripts": {
    "release": "npm run prettier && npm run lint && npm run build",
    "build": "webpack --env build",
    "build:dev": "webpack --env dev",
    "watch": "npm run build -- --watch",
    "watch:dev": "npm run build:dev -- --watch",
    "lint": "eslint ./src --fix",
    "prettier": "prettier --write './src/**/*.{vue,js,scss,html}'"
  },
  "dependencies": {
    "vue": "^2.5.21"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.2.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.11.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-vue": "^5.0.0",
    "extensionizer": "^1.0.1",
    "node-sass": "^4.11.0",
    "prettier": "^1.15.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "vue-eslint-parser": "^4.0.3",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.5.21",
    "webpack": "^4.28.1",
    "webpack-chrome-extension-reloader": "^1.1.1",
    "webpack-cli": "^3.1.2"
  },
  "browserslist": [
    "> 0.25%",
    "not ie 11",
    "not op_mini all"
  ]
}
